{
    "compilerOptions": {
        "outDir": "./build",
		"target": "ES6",
		"module": "ES6",
		"moduleResolution": "node",
		"strict": true,
		"declaration": true,
		"esModuleInterop": true,
		"allowJs": false,
		"noImplicitAny": false,
		"strictPropertyInitialization": false
	},
    "include": [
		"lib"
    ],
    "exclude": [
        "node_modules",
		"external",
		"test",
    ]
}
